4.sum of diagonal matrix or trace of matrix of nxn matrix
// Online C compiler to run C program online
#include <stdio.h>
int main() {
 int n,s=0;
 scanf("%d",&n);
 int a[n][n];
  int i,j;
  for(i=0;i<n;i++)
  {
      for(j=0;j<n;j++)
      scanf("%d",&a[i][j]);
  }
  for(i=0;i<n;i++)
   {
       for(j=0;j<n;j++)
      {
        if(i==j)
        s=s+a[i][j];
      }
   }
  printf("%d",s);
       return 0;
}
