2.Transpose of matrix(mxn)
// Online C compiler to run C program online
#include <stdio.h>
int main() {
 int m,n;
 scanf("%d%d",&m,&n);
 int a[m][n];
  int i,j;
  for(i=0;i<m;i++)
  {
      for(j=0;j<n;j++)
      scanf("%d",&a[i][j]);
  }
  for(i=0;i<m;i++)
  {
      for(j=0;j<n;j++)
      printf("%d",a[j][i]);
      printf("\n");
  }
       return 0;
}

