
###print numbers from 1-5
code:
#include <stdio.h>
int main() {
   int a=1;
   while(a<=5)
   { 
       printf("%d\n",a);
       a++;  //a=a+1
   }
    return 0;
} 

OUTPUT:
1
2
3
4
5
---------------------------------------------------------------
###print numbers from 5-1
code:
#include <stdio.h>
int main() {
   int a=5;
   while(a>=5)
   { 
       printf("%d\n",a);
       a--;  //a=a-1
   }
    return 0;
} 


OUPUT:
5
4
3
2
1
--------------------------------------------------------------------
###print numbers from 1-n
code:
#include <stdio.h>
int main() {
   int a=1,n;
   scanf("%d",&n);
   while(a<=n)
   { 
       printf("%d\n",a);
       a++;  //a=a+1
   }
    return 0;
} 

CODE:
5
1
2
3
4
5
-------------------------------------------------------------------
###print numbers from 1-n
code:
#include <stdio.h>
int main() {
   int a=1,n;
   scanf("%d",&n);
   while(a<=n)
   { 
       printf("%d\n",a);
       a=a+2;
   }
    return 0;
}

CODE:
10
1 
3
5
7
9
-------------------------------------------------------------------
###print  sum of  natural numbers from 1-n
code:
#include <stdio.h>
int main() {
   int a=1,n,s=0;
   scanf("%d",&n);
   while(a<=n)
   { 
     s=s+a;
     a=a+1;
  }
printf("%d\n",s);
    return 0;
}

OUTPUT:
5
15
------------------------------------------------------------------------
###print  multiplication of numbers from 1-n
code:
#include <stdio.h>
int main() {
   int a=1,n,s=1;
   scanf("%d",&n);
   while(a<=n)
   { 
     s=s*a;
     a=a+1;
  }
printf("%d\n",s);
    return 0;
}

OUTPUT:
5
120
---------------------------------------------------------------------------
