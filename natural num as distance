// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int i,n,r=0;
    scanf("%d",&n);
    for(i=1;;i++)
    {
        printf("%d, ",r);
        r=n+i;
        n=r;
    }

    return 0;
}
